#include<stdio.h>
#include "main.h"
/**
 * _strpbrk - returns the characters that semilar in the accept string
 * @s: the first string
 * @accept: the second string
 * Return: the string has similar characters
 */
char *_strpbrk(char *s, char *accept)
{


  while (*s != '\0')
    {
      if (*s == *accept)
	return (s);
      s++;
    }
  if (*accept == '\0')
  return (s);
return (NULL);
}
